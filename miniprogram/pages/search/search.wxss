
.search-flag-box{
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    margin-top: 15rpx;
    margin-bottom: 15rpx;
    height: 90rpx;
    background-color: #ffffff;
}

.search-left-box{
    display: flex;
    align-items: center;
    padding: 0 15rpx;
    background-color: transparent;
    width: 100%;
    height: 100%;
}
.search-left-box tag{
    margin: 0 15rpx;
    font-size: 40rpx;
}
.icon-more{
    position: absolute;
    right: 30rpx;
    color: #262626;
    height: 100%;
    font-size: 60rpx;
    line-height: 90rpx;
    background-color: white;
}
historyTag{
    transform: translate(2%,0);
}