page{
    padding: 0 30rpx;
}
.my-module,
.more-module{
    height: 400rpx;
    background-color: transparent;
}
.my-module-title,
.more-module-title{
    display: flex;
    text-align: center;
    background-color: transparent;
    margin-top: 70rpx;
    font-size: 33rpx;
}
.icon-delete{
    position: absolute;
    right: 20rpx;
    top: 20rpx;
    font-size: 60rpx;
}
.more-module{
    margin-top: 50rpx;
}

.more-module-title{
    margin-top: 20rpx;
}

.module-tag-box{
    padding: 20rpx 0;
    display: flex;
}
.module-tag-box tag{
    margin: 10rpx 20rpx;
}